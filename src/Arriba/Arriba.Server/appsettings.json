{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ArribaServer": {
    "EnabledAuthentication": false,
    "FrontendBaseUrl": "https://localhost:8080/",
    "OAuthConfig": {
      "RedirectUrl": "https://localhost:8080/data/api/oauth/auth-code",
      "TenantId": "<AZURE AD TENANT ID>",
      "AudienceId": "<AZURE AD APP CLIENT ID>",
      "Prompt": "login",
      "Scopes": [ "openid", "<AZURE AD APP CLIENT ID>/.default" ]
    }
  }
}
