{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ArribaServer": {
    "EnabledAuthentication": false,
    "FrontendBaseUrl": "http://localhost:8080/",
    "OAuthConfig": {
      "RedirectUrl": "http://localhost:8080/data/api/oauth/auth-code",
      "TenantId": "",
      "AudienceId": "",
      "Prompt": "login",
      "Scopes": [ "openid", "{AudienceId}/.default", "offline_access" ]
    }
  }
}
